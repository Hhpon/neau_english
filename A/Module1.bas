Function GetPara(pStr As String, Para As String, Optional NeedEncode As Boolean = True) As String
Dim i As Integer
    
    i = InStr(pStr, "<" & Para & ">")
    
    If i = 0 Then GetPara = "": Exit Function
    
    GetPara = Mid(pStr, i + Len("<" & Para & ">"))
    
    i = InStr(GetPara, "</" & Para & ">")
    
