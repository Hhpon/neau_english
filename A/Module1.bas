Function SoapPost(Action As String, Total As Integer) As String
     Dim XmlHttp As Object, Para, i As Integer
     Set XmlHttp = CreateObject("Msxml2.XMLHTTP")
     If Not IsObject(XmlHttp) Then
         Set XmlHttp = CreateObject("Microsoft.XMLHTTP")
         If Not IsObject(XmlHttp) Then Exit Function
     End If
     
     XmlHttp.Open "POST", "http://" & IP & "/WebService/ServiceV3.asmx", False
     
     XmlHttp.setRequestHeader "CONTENT-TYPE", "text/xml; charset=utf-8"
     XmlHttp.setRequestHeader "SOAPAction", "http://www.open.com.cn/" & Action
     
     
    Para = "<?xml version=""1.0"" encoding=""utf-8""?>" & _
    "<soap:Envelope xmlns:xsi=""http://www.w3.org/2001/XMLSchema-instance"" xmlns:xsd=""http://www.w3.org/2001/XMLSchema"" xmlns:soap=""http://schemas.xmlsoap.org/soap/envelope/"">" & _
    "<soap:Header>" & _
    "<CredentialSoapHeader xmlns=""http://www.open.com.cn"">" & _
      "<Username>" & EnCode(UserName) & "</Username>" & _
      "<Password>" & EnCode(PassWord) & "</Password>" & _
      "<ClientCredential>83BCC79913F2331685F4851C4ED2DA72  E37438347941F22F6A873D18CE562AC9</ClientCredential>" & _
    "</CredentialSoapHeader>" & _
  "</soap:Header>" & _
  "<soap:Body>" & _
    "<" & Action & " xmlns=""http://www.open.com.cn"">"
    
      For i = 0 To Total - 1
        If PNeedEncode(i) Then
            Para = Para & "<" & PName(i) & ">" & EnCode(PVal(i)) & "</" & PName(i) & ">"
        Else
            Para = Para & "<" & PName(i) & ">" & PVal(i) & "</" & PName(i) & ">"
        End If
      Next
      
    Para = Para & "</" & Action & "></soap:Body></soap:Envelope>"
     
     
     XmlHttp.Send Para
     Do While XmlHttp.readyState <> 4
         DoEvents
     Loop

     If XmlHttp.Status = 200 Then
         SoapPost = XmlHttp.responseText
         
